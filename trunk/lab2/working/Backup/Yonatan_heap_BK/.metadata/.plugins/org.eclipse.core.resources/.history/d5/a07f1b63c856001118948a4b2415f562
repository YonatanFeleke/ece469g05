#include "usertraps.h"


void main (int x)
{
	int *malloc_address = malloc(4);
	int value = 5;
	*(malloc_address)= value;
	

  // TEST #6: TEST HEAP FUNCTIONALITY
  //////////////////////////////////////////////////////////////////////
  Printf("\n-------------------------------------------------------------------------------------\n");
	Printf("Starting to call heap\n");
	malloc_address = (int *) malloc(4);
	Printf("Malloc returned with value 0x%.8X\n",(int)malloc_address);
	*malloc_address = value;
	Printf("Value at 0x%.8X:0x%.8X\n",(int)malloc_address,&malloc_address);
  //////////////////////////////////////////////////////////////////////

}

